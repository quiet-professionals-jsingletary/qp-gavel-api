{
  "name": "qp-ampd-app",
  "version": "1.0.0",
  "author": "James Singletary IV",
  "description": "ExpressJS RESTful API Server built on top of RESTful API serving up data for GAVEL",
  "main": "server.js",
  "type": "commonjs",
  "dependencies": {
    "axios": "^0.21.1",
    "cors": "^2.8.5",
    "dotenv": "^8.6.0",
    "express": "^4.17.1",
    "helmet": "^4.6.0",
    "node-fetch": "^2.6.1",
    "node-rsa": "^1.1.1",
    "pm2": "^5.1.0",
    "proxy-addr": "^2.0.7"
  },
  "devDependencies": {
    "ncp": "^2.0.0",
    "nodemon": "^2.0.12",
    "sloc": "^0.2.1"
  },
  "engines": {
    "node": ">=14.15.1",
    "npm": ">=6.14.8"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Quiet-Professionals-LLC/qp-gavel-api.git"
  },
  "keywords": [
    "GAVEL",
    "Mobile Device Data",
    "QP",
    "Quiet Professionals LLC",
    "Venntel"
  ],
  "scripts": {
    "start": "nodemon server",
